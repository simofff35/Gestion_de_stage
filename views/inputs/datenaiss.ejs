<div class="form-group row">
  <div class="col-sm-6 mb-3 mb-sm-0">
    <input
      type="tel"
      class="form-control form-control-user"
      id=""
      placeholder="Numéro de téléphone"
      minlength="10"
      maxlength="10"
      pattern="[0-9]+"
      title="Veuillez entrer un numéro de téléphone valide (10 chiffres)."
      name="tel"
    />
  </div>
  <div class="col-sm-6">
    <input type="date" class="form-control form-control-user" id=""
    name="datenaiss" placeholder="Date de naissance" required max="2005-01-01"
    // Set the maximum date to limit it to 18 years old or above />
  </div>
</div>
